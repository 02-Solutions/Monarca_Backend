{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^seed.service$": "<rootDir>/seed.service",
    "^src/(.*)$": "<rootDir>/src/$1"
  },
  "moduleDirectories": ["node_modules", "src"],
  "setupFiles": ["dotenv/config"],
  "testTimeout": 30000
}
